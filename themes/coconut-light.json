{
    "name": "Coconut Light",
    "type": "light",
    "colors": {
        "editor.background": "#FDFBF7",
        "editor.foreground": "#4A3B32",
        "activityBar.background": "#F0EBE5",
        "activityBar.foreground": "#4A3B32",
        "sideBar.background": "#F7F3EE",
        "sideBar.foreground": "#4A3B32",
        "statusBar.background": "#7CB342",
        "statusBar.foreground": "#FFFFFF",
        "editorGroupHeader.tabsBackground": "#F0EBE5",
        "tab.activeBackground": "#FDFBF7",
        "tab.inactiveBackground": "#F0EBE5",
        "tab.activeForeground": "#4A3B32",
        "list.activeSelectionBackground": "#E0F2F1",
        "list.activeSelectionForeground": "#4A3B32",
        "list.hoverBackground": "#F1F8E9",
        "titleBar.activeBackground": "#FDFBF7",
        "titleBar.activeForeground": "#4A3B32"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#A1887F",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#7CB342",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string",
                "string.template",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#29B6F6"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#FDD835"
            }
        },
        {
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#4A3B32"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#00897B"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#F9A825"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#7CB342"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#29B6F6"
            }
        }
    ]
}