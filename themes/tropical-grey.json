{
    "name": "Tropical Grey",
    "type": "dark",
    "colors": {
        "editor.background": "#252A2E",
        "editor.foreground": "#E0F7FA",
        "activityBar.background": "#1C2023",
        "activityBar.foreground": "#E0F7FA",
        "sideBar.background": "#202428",
        "sideBar.foreground": "#E0F7FA",
        "statusBar.background": "#00E676",
        "statusBar.foreground": "#252A2E",
        "editorGroupHeader.tabsBackground": "#1C2023",
        "tab.activeBackground": "#252A2E",
        "tab.inactiveBackground": "#1C2023",
        "tab.activeForeground": "#E0F7FA",
        "list.activeSelectionBackground": "#37474F",
        "list.activeSelectionForeground": "#00E676",
        "list.hoverBackground": "#2F363B",
        "titleBar.activeBackground": "#252A2E",
        "titleBar.activeForeground": "#E0F7FA"
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#78909C",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#00E676",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "string",
                "string.template",
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#4DD0E1"
            }
        },
        {
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#CDDC39"
            }
        },
        {
            "scope": [
                "variable",
                "support.variable"
            ],
            "settings": {
                "foreground": "#E0F7FA"
            }
        },
        {
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#4DD0E1"
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.other.inherited-class",
                "support.class"
            ],
            "settings": {
                "foreground": "#CDDC39"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#00E676"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#4DD0E1"
            }
        }
    ]
}